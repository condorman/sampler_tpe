{
  "name": "optuna-tpe-js",
  "version": "0.2.0",
  "description": "Optuna-compatible TPE sampler and Study primitives for JavaScript.",
  "type": "module",
  "main": "./src/optuna_tpe.js",
  "exports": {
    ".": "./src/optuna_tpe.js"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "test": "vitest run tpeCore.golden.test.js --testTimeout=120000",
    "test:golden": "vitest run tpeCore.golden.test.js --testTimeout=120000",
    "golden:generate": "python3 generate_tpe_golden.py",
    "pack:check": "npm pack --dry-run",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "optuna",
    "tpe",
    "hyperparameter-optimization",
    "sampler"
  ],
  "license": "GPL-3.0-only",
  "devDependencies": {
    "vitest": "^4.0.18"
  }
}
